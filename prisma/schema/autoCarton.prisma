model autoCartonFaults {
    ID         Int     @id @default(autoincrement())
    timestamp  DateTime
    line      Int
    machineType autoCartonMachineType
    faultCode  Int
}

model faultCodeLookup {
    ID           Int     @id @default(autoincrement())
    faultMessage String
}

enum autoCartonMachineType {
   Lidder
   iPack
   labeler
   erector
   barcoder
}